        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Transaction"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://resourceprojects.org/def/description -->

    <owl:DatatypeProperty rdf:about="http://resourceprojects.org/def/description">
        <rdfs:label xml:lang="en">Description
</rdfs:label>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Source"/>
    </owl:DatatypeProperty>
    


    <!-- http://resourceprojects.org/def/endDate -->

    <owl:DatatypeProperty rdf:about="http://resourceprojects.org/def/endDate">
