    </owl:DatatypeProperty>
    


    <!-- http://resourceprojects.org/def/archiveCopy -->

    <owl:DatatypeProperty rdf:about="http://resourceprojects.org/def/archiveCopy">
        <rdfs:label xml:lang="en">Archive Copy</rdfs:label>
        <rdfs:comment xml:lang="en">Sometimes original source documents may disappear from the web. 

This field is used to link to a long-term archival copy of a source document. </rdfs:comment>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Source"/>
    </owl:DatatypeProperty>
    


    <!-- http://resourceprojects.org/def/basisOfAccounting -->

    <owl:DatatypeProperty rdf:about="http://resourceprojects.org/def/basisOfAccounting">
        <rdfs:label xml:lang="en">Basis of Accounting
</rdfs:label>
        <rdfs:comment xml:lang="en">A basis of accounting can be defined as the time various financial transactions are recorded. The &apos;cash basis&apos; and the &apos;accrual basis&apos; are the two primary methods of tracking income and expenses in accounting.

Valid values for this field are &apos;cash&apos;, &apos;accural&apos;, &apos;unknown&apos; and &apos;other&apos;.
</rdfs:comment>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/CompanyPayment"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/GovernmentReceipt"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Transaction"/>
    </owl:DatatypeProperty>
    


    <!-- http://resourceprojects.org/def/currency -->

    <owl:DatatypeProperty rdf:about="http://resourceprojects.org/def/currency">
        <rdfs:label xml:lang="en">Currency</rdfs:label>
        <rdfs:comment xml:lang="en">The currency of a transaction, using ISO 4217 three-letter currency codes.

See https://en.wikipedia.org/wiki/ISO_4217 for a list of codes. </rdfs:comment>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/CompanyPayment"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/GovernmentReceipt"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Transaction"/>
    </owl:DatatypeProperty>
    


    <!-- http://resourceprojects.org/def/date -->

    <owl:DatatypeProperty rdf:about="http://resourceprojects.org/def/date">
