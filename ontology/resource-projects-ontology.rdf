    <!-- http://resourceprojects.org/def/CEO -->

    <owl:Class rdf:about="http://resourceprojects.org/def/CEO">
        <rdfs:subClassOf rdf:resource="http://resourceprojects.org/def/Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/source"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Source"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://resourceprojects.org/def/Commodity -->

    <owl:Class rdf:about="http://resourceprojects.org/def/Commodity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/source"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Source"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://resourceprojects.org/def/Company -->

    <owl:Class rdf:about="http://resourceprojects.org/def/Company">
        <rdfs:subClassOf rdf:resource="http://resourceprojects.org/def/Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/source"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Source"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Company is used to represent any kind of organization that can enter into transactions around a resource project, license, contract or site, with the exception of government parties, which are represented by GovernmentParty.

Government owned companies are still represented using Company. 
</rdfs:comment>
    </owl:Class>
    


    <!-- http://resourceprojects.org/def/CompanyPayment -->

    <owl:Class rdf:about="http://resourceprojects.org/def/CompanyPayment">
        <rdfs:subClassOf rdf:resource="http://resourceprojects.org/def/Transaction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/payee"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Country"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/relatedProject"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Project"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/paymentType"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/PaymentType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/payer"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Company"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/source"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Source"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/payee"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/GovernmentParty"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://resourceprojects.org/def/Contract -->

    <owl:Class rdf:about="http://resourceprojects.org/def/Contract">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/hasStake"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Stake"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/source"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Source"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://resourceprojects.org/def/Contributor -->

