    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://resourceprojects.org/def/"
     xml:base="http://resourceprojects.org/def/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://resourceprojects.org/def/"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resourceprojects.org/def/commodity -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/commodity">
        <rdfs:comment xml:lang="en">Relates a project, site or license to a commodity. </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/groupMember -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/groupMember">
        <rdfs:comment xml:lang="en">Used to relate a group to it&apos;s member companies. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://resourceprojects.org/def/member"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/hasLocation -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/hasLocation">
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Company"/>
        <rdfs:range rdf:resource="http://resourceprojects.org/def/Country"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Group"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/License"/>
        <rdfs:range rdf:resource="http://resourceprojects.org/def/Location"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Organization"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Project"/>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Site"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/hasStake -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/hasStake">
        <rdfs:comment xml:lang="en">Relates an entity to stakes that exist within it. 

For example, if a company owns a stake in a project, then:

&lt;Project&gt; hasStake &lt;Stake&gt;.
&lt;Stake&gt; hasStakeholder &lt;Company&gt;.</rdfs:comment>
        <rdfs:range rdf:resource="http://resourceprojects.org/def/Stake"/>
        <rdfs:subPropertyOf rdf:resource="http://resourceprojects.org/def/stakeholder"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/hasStakeholder -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/hasStakeholder">
        <rdfs:comment xml:lang="en">Relates a stake to the organization or other entity holding that stake. </rdfs:comment>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Stake"/>
        <rdfs:subPropertyOf rdf:resource="http://resourceprojects.org/def/stakeholder"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/hasStakeholding -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/hasStakeholding">
        <rdfs:comment xml:lang="en">Relates an entity to the stake it holds. 

For example:

&lt;Company&gt; hasStakeholding &lt;Stake&gt;.
&lt;Stake&gt; isStakeIn &lt;Project&gt;.</rdfs:comment>
        <rdfs:range rdf:resource="http://resourceprojects.org/def/Stake"/>
        <owl:inverseOf rdf:resource="http://resourceprojects.org/def/hasStakeholder"/>
        <rdfs:subPropertyOf rdf:resource="http://resourceprojects.org/def/stakeholding"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/isStakeIn -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/isStakeIn">
        <rdfs:comment xml:lang="en">Relates a stake to the entity it is a stake in. </rdfs:comment>
        <rdfs:domain rdf:resource="http://resourceprojects.org/def/Stake"/>
        <owl:inverseOf rdf:resource="http://resourceprojects.org/def/hasStake"/>
        <rdfs:subPropertyOf rdf:resource="http://resourceprojects.org/def/stakeholding"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/member -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/member">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/organisation -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/organisation">
        <rdfs:subPropertyOf rdf:resource="http://resourceprojects.org/def/member"/>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/payee -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/payee"/>
    


    <!-- http://resourceprojects.org/def/payer -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/payer"/>
    


    <!-- http://resourceprojects.org/def/paymentType -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/paymentType"/>
    


    <!-- http://resourceprojects.org/def/productionStatistics -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/productionStatistics"/>
    


    <!-- http://resourceprojects.org/def/relatedProject -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/relatedProject">
        <rdfs:comment xml:lang="en">Relates transactions and production statistics to projects. </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/source -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/source"/>
    


    <!-- http://resourceprojects.org/def/stakeholder -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/stakeholder">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Transitive parent property of hasStake and hasStakeholder.

This makes it possible to query for all the ultimate stakeholders in a given entity. </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/stakeholding -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/stakeholding">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Transitive parent property of hasStakeholding and isStakeIn.

This makes it possible to query for all the ultimate stakeholdings of a given entity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://resourceprojects.org/def/wasAttributedTo -->

    <owl:ObjectProperty rdf:about="http://resourceprojects.org/def/wasAttributedTo">
        <rdfs:label xml:lang="en">Source attributed to</rdfs:label>
        <rdfs:comment xml:lang="en">A source may be attributed to any number of individuals or organisations, including the ultimate originator of the documents, and individuals involved in processing the document for its inclusion on the ResourceProjects.org platform. </rdfs:comment>
    </owl:ObjectProperty>
    


