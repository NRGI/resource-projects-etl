            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resourceprojects.org/def/hasLocation"/>
                <owl:someValuesFrom rdf:resource="http://resourceprojects.org/def/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


